 void setZeroes(vector<vector<int>>& matrix) {
        int colf=1, row= matrix.size(), col=matrix[0].size();
        
    // row-wise traversal    
        for(int i=0;i<row;i++)
        {  if(matrix[i][0]==0) colf=0;
         
           for(int j=1;j<col;j++)
           {
               if(matrix[i][j]==0)
               {
                   matrix[i][0]=matrix[0][j]=0;
               }
           }
         
        }
        
   
    // Backward traversal
        for(int i=row-1;i>=0;i--)
        {
            for(int j=col-1;j>=1;j--)
            {
                if(matrix[i][0]==0 || matrix[0][j]==0)
                    matrix[i][j]=0;                  
            }
            if(colf==0)
                    matrix[i][0]=0;
        }  
        
    }
